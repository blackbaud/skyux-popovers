{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-popovers",
  "mode": "easy",
  "compileMode": "aot",
  "host": {
    "url": "https://developer.blackbaud.com",
    "frameOptions": {
      "none": true
    }
  },
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "omnibar": {},
  "params": {
    "svcid": {
      "value": "skyux"
    }
  },
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "app": {
    "styles": [
      "@skyux/docs-tools/css/docs-tools.css"
    ],
    "theming": {
      "supportedThemes": [
        "default",
        "modern"
      ],
      "theme": "default"
    }
  }
}
