{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-popovers",
  "mode": "easy",
  "compileMode": "aot",
  "skyuxModules": [
    "SkyErrorModule"
  ],
  "moduleAliases": {
    "@skyux/popovers": "src/app/public"
  },
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "app": {
    "styles": [
      "src/app/visual-styles.scss",
      "@skyux/theme/css/sky.css"
    ]
  }
}
